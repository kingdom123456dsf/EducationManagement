<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springboot.dao.TeacherfacultiesDao">

    <!--    sql语句-->
    <select id="findBySearch" resultType="com.example.springboot.entity.Teacherfaculties">
        select `teacherfaculties`.* from `teacherfaculties`

        where 1 = 1
        <if test="search != null and search.Dno != null and search.Dno != ''">
            and `teacherfaculties`.`Dno` like concat ('%', #{search.Dno}, '%')
        </if>

    </select>

</mapper>